{
  "env": {
    "test": {
      "presets": [
        [ "@babel/preset-env" ],
        ["@babel/preset-react", {"runtime": "automatic"}]
      ]
    }
  },
  "presets": [
    "react-app",
    [
      "@babel/preset-env",
      { "modules": false }
    ],
    ["@babel/preset-react", {"runtime": "automatic"}]
  ],
  "plugins": [
    [ "@babel/plugin-transform-runtime",
      { "regenerator": true }
    ],
    [ "@babel/plugin-proposal-class-properties",
      { loose: true }
    ],
    [
      "@babel/plugin-transform-react-jsx",
      { "throwIfNamespace": false, "runtime": "automatic" }
    ]
  ]
}
